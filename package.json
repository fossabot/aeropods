{
	"name": "@araclx/aeropods",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev:build:d": "docker-compose -f 'docker-compose.dev.yml' up -d --build",
		"dev:start:d": "docker-compose -f 'docker-compose.dev.yml' up -d",
		"dev:tools": "yarn workspace @aeropods/client run devtools",
		"webpack-defaults": "webpack-defaults"
	},
	"devDependencies": {
		"lerna": "^3.18.5",
		"prettier": "1.19.1",
		"prettier-plugin-packagejson": "^2.0.10"
	}
}
